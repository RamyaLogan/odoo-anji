<odoo>
  <record id="view_users_form_smartflo" model="ir.ui.view">
    <field name="name">res.users.smartflo.form</field>
    <field name="model">res.users</field>
    <field name="inherit_id" ref="base.view_users_form"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='signature']" position="after">
        <field name="smartflo_account_type"/>
        <field name="smartflo_extension_number"/>
        <field name="smartflo_agent_number"/>
        <field name="smartflo_caller_id"/>
      </xpath>
    </field>
  </record>
</odoo>