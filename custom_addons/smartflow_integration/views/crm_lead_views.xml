<odoo>
  <record id="view_crm_lead_form_inherit_smartflo" model="ir.ui.view">
    <field name="name">crm.lead.form.inherit.smartflo</field>
    <field name="model">crm.lead</field>
    <field name="inherit_id" ref="crm.crm_lead_view_form"/>
    <field name="arch" type="xml">
      <xpath expr="//sheet/notebook" position="inside">
        <page string="Call History">
          <field name="smartflo_call_log_ids" mode="tree,form" context="{'default_lead_id': active_id}">
            <tree string="Call Logs" default_order="effective_start_time desc">
              <field name="agent_id"/>
              <field name="status"/>
              <field name="start_time"/> 
              <field name="requested_time"/>      
              <field name="duration"/>
              <field name="direction"/>
            </tree>
          </field>
        </page>
      </xpath>
    </field>
  </record>
</odoo>